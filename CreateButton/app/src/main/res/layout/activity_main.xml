<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context="io.mda.createbutton.MainActivity">    <android.support.v7.widget.AppCompatButton        android:id="@+id/raiseButton"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_margin="16dp"        android:layout_centerHorizontal="true"        android:text="@string/btn_elevado"        style="@style/RaiseButton"/>        <android.support.v7.widget.AppCompatButton        android:id="@+id/flatButton"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_margin="16dp"        android:layout_centerHorizontal="true"        android:layout_below="@+id/raiseButton"        android:text="@string/flat_button"        style="@style/FlatButton"/>    <android.support.design.widget.FloatingActionButton        android:id="@+id/fab"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_margin="16dp"        android:src="@drawable/ic_fab"        android:layout_alignParentBottom="true"        android:layout_alignParentEnd="true"        app:fabSize="normal"        android:backgroundTint="@color/colorPrimary"        android:elevation="8dp"/></RelativeLayout>